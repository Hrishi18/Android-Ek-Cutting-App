<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.hrishikeshgawas.ekcutting">

    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.hrishikeshgawas.ekcutting.AttributionActivity"/>
        <activity android:name="com.hrishikeshgawas.ekcutting.SplashScreen"
            android:theme="@style/SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.hrishikeshgawas.ekcutting.MainActivity"
            android:theme="@style/AppTheme"/>
        <activity android:name="com.hrishikeshgawas.ekcutting.DetailsActivity"/>
        <activity android:name="com.hrishikeshgawas.ekcutting.NonVegDetailsActivity"/>
    </application>


</manifest>